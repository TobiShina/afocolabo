      },
      "edgeToEdgeEnabled": true,
      "package": "com.makaay120.afofinance"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1c18a850-9bf0-429e-8a08-8f193fa18519"
      }
    }
